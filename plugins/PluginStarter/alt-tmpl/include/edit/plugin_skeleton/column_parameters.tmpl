$('.close-plugin').click(function(){
    if( !confirm('<mt:trans phrase="Are you sure you want to remove this item?" component="PluginStarter">') ) {
        return false;
    }
    var childCnt = $(this).parent().parent().parent().children().length;
    if ( childCnt == 2 ) {
        $(this).parent().parent().parent().parent().hide();
    }
    $(this).parent().parent().remove();
});
$('.callback_model-selector').change(function(){
    let selModel   = $(this).val();
    let parentNode = $(this).parent().next();
    let selector   = parentNode.children( 'select' );
    let selOption  = selector.children( '.404-error' );
    if ( selModel == 'template' ) {
        selOption.show();
    } else {
        selOption.hide();
    }
});
$('.hook_kind').change(function(){
    var selected_kind = $(this).val();
    var same_kind = 0;
    $('.hook_kind').each(function(){
        if ( $(this).val() == selected_kind ) {
            same_kind++;
        }
    });
    if ( same_kind > 1 ) {
        alert( '<mt:trans phrase="You cannot register the same type of hook within a Plug in."  component="PluginStarter">' );
        $(this).val('');
    }
});
