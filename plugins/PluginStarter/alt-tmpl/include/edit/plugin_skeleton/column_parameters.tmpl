<style>
thead{ background-color:black; color: white }
.close-plugin{ padding:0 5px; margin-left:0;margin-right:0 }
thead tr { border-bottom: 3px solid #444 !important}
tr { border: 1px solid #444 !important}
td { border-top: 1px solid #444 !important}
/*
input[type="text"]{ padding: 5px }
*/
</style>
<mt:if name="user_language" ne="en">
<div class="row form-group">
  <div class="col-lg-2">
    <label for="description_locale">
      <mt:trans phrase="Description"> (<mt:var name="language" translate>)
    </label>
  </div>
  <div class="col-lg-10" style="overflow: auto">
    <input type="text" id="description_locale" name="description_locale" class="form-control watch-changed" value="<mt:var name="request.description_locale" escape>">
  </div>
</div>
</mt:if>

<hr>
<div class="row form-group mt-4">
  <div class="col-lg-2">
    <label for="has_app">
    <mt:trans phrase="Extended Apps">
    </label>
  </div>
  <div class="col-lg-10">
    <input type="hidden" name="has_app" value="0">
    <label class="custom-control custom-checkbox">
    <input id="has_app" class="custom-control-input watch-changed"
     type="checkbox" name="has_app" value="1" <mt:if name="object_has_app">checked</mt:if>>
        <span class="custom-control-indicator"></span>
        <span class="custom-control-description"> 
        <mt:trans phrase="Export apps that extends class Prototype." component="PluginStarter"></span>
    </label>
  </div>
</div>
<hr>

<mt:var name="request.config_key" setvar="config_keys">
<mt:var name="request.config_value" setvar="config_values">
<mt:arrayslice name="config_keys" offset="1">
<mt:arrayslice name="config_values" offset="1">
<div class="row form-group">
  <div class="col-lg-2">
    <label>
    <button id="add_config" class="btn btn-sm  pt-0 pb-0 pl-0 pr-0 mb-1 ml-0" type="button">
      <i class="fa fa-plus-circle" aria-hidden="true"></i><span class="sr-only"><mt:trans phrase="Add"></span>
    </button>
    <mt:trans phrase="Config Settings" component="PluginStarter">
    </label>
  </div>
  <div class="col-lg-10" style="overflow: auto" style="overflow: auto">
    <table id="config_table" <mt:unless name="config_keys">style="display:none"</mt:unless> class="table table-striped table-sm table-hover mb-1" style="min-width:800px; overflow: auto;">
      <thead>
        <tr>
          <th class="text-center" class="text-center" style="width:50%"><mt:trans phrase="Name"></th>
          <th class="text-center" class="text-center" style="width:50%"><mt:trans phrase="Value"></th>
          <th></th>
        </tr>
      </thead>
      <tbody id="config_body">
        <tr id="config_tmpl" style="display:none">
          <td style="width:50%">
            <input type="text" name="config_key[]" class="form-control watch-changed form-control-sm" value="">
          </td>
          <td style="width:50%">
            <input type="text" name="config_value[]" class="form-control watch-changed form-control-sm" value="">
          </td>
          <td>
          <button class="btn btn-secondary btn-sm close-plugin mt-1 mr-2" type="button" aria-label="<mt:trans phrase="Detach">">
            <span aria-hidden="true">&times;</span>
          </button>
          </td>
        </tr>
        <mt:loop name="config_keys">
        <tr>
          <td>
            <input type="text" name="config_key[]" class="form-control watch-changed form-control-sm" value="<mt:var name="__value__" escape>">
          </td>
          <td>
            <mt:setvarblock name="value_name">config_values[<mt:var name="__index__">]</mt:setvarblock>
            <input type="text" name="config_value[]" class="form-control watch-changed form-control-sm" value="<mt:var name="$value_name" escape>">
          </td>
          <td>
          <button class="btn btn-secondary btn-sm close-plugin mt-1 mr-2" type="button" aria-label="<mt:trans phrase="Detach">">
            <span aria-hidden="true">&times;</span>
          </button>
          </td>
        </tr>
        </mt:loop>
      </tbody>
    </table>
  </div>
</div>

<mt:var name="request.setting_key" setvar="setting_keys">
<mt:var name="request.setting_value" setvar="setting_values">
<mt:var name="request.setting_scope" setvar="setting_scopes">
<mt:var name="request.setting_label" setvar="setting_labels">
<mt:var name="request.setting_label_locale" setvar="setting_label_locales">
<mt:arrayslice name="setting_keys" offset="1">
<mt:arrayslice name="setting_values" offset="1">
<mt:arrayslice name="setting_scopes" offset="1">
<mt:arrayslice name="setting_labels" offset="1">
<mt:arrayslice name="setting_label_locales" offset="1">
<div class="row form-group">
  <div class="col-lg-2">
    <label>
    <button id="add_setting" class="btn btn-sm pt-0 pb-0 pl-0 pr-0 mb-1 ml-0" type="button">
      <i class="fa fa-plus-circle" aria-hidden="true"></i><span class="sr-only"><mt:trans phrase="Add"></span>
    </button>
    <mt:trans phrase="Plugin Settings" component="PluginStarter">
    </label>
  </div>
  <div class="col-lg-10" style="overflow: auto">
    <table id="setting_table" <mt:unless name="setting_keys">style="display:none"</mt:unless> class="table table-striped table-sm table-hover mb-1" style="min-width:800px; overflow: auto;">
      <thead>
        <tr>
          <th class="text-center" class="text-center" style="width:20%"><mt:trans phrase="Name"></th>
          <th class="text-center" class="text-center" style="width:20%"><mt:trans phrase="Value"></th>
          <th class="text-center" class="text-center" style="width:16%"><mt:trans phrase="Scope" component="PluginStarter"></th>
          <th class="text-center" class="text-center" style="width:<mt:if name="user_language" ne="en">22%<mt:else>44%</mt:if>"><mt:trans phrase="Label"></th>
          <mt:if name="user_language" ne="en">
          <th class="text-center" class="text-center" style="width:22%"><mt:trans phrase="Label"> (<mt:var name="language" translate>)</th>
          </mt:if>
          <th></th>
        </tr>
      </thead>
      <tbody id="setting_body">
        <tr id="setting_tmpl" style="display:none">
          <td style="width:20%">
            <input type="text" name="setting_key[]" class="form-control watch-changed form-control-sm" value="">
          </td>
          <td style="width:20%">
            <input type="text" name="setting_value[]" class="form-control watch-changed form-control-sm" value="">
          </td>
          <td style="width:16%">
            <select class="form-control watch-changed form-control-sm custom-select" name="setting_scope[]"
              style="height:31px;">
              <option value="0"><mt:trans phrase="None"></option>
              <option value="1"><mt:trans phrase="System"></option>
              <option value="2"><mt:trans phrase="WorkSpace"></option>
              <option value="3"><mt:trans phrase="Both" component="PluginStarter"></option>
            </select>
          </td>
          <td style="width:<mt:if name="user_language" ne="en">22%<mt:else>44%</mt:if>">
            <input type="text" name="setting_label[]" class="form-control watch-changed form-control-sm" value="">
          </td>
          <mt:if name="user_language" ne="en">
          <td style="width:22%">
          <input type="text" name="setting_label_locale[]" class="form-control watch-changed form-control-sm" value="">
          </td>
          </mt:if>
          <td>
          <button class="btn btn-secondary btn-sm close-plugin mt-1 mr-2" type="button" aria-label="<mt:trans phrase="Detach">">
            <span aria-hidden="true">&times;</span>
          </button>
          </td>
        </tr>
        <mt:loop name="setting_keys">
        <tr>
          <td style="width:20%">
            <input type="text" name="setting_key[]" class="form-control watch-changed form-control-sm" value="<mt:var name="__value__" escape>">
          </td>
          <td style="width:20%">
            <mt:setvarblock name="value_name">setting_values[<mt:var name="__index__">]</mt:setvarblock>
            <input type="text" name="setting_value[]" class="form-control watch-changed form-control-sm" value="<mt:var name="$value_name" escape>">
          </td>
          <td style="width:16%">
            <mt:setvarblock name="value_name">setting_scopes[<mt:var name="__index__">]</mt:setvarblock>
            <select class="form-control watch-changed form-control-sm custom-select" name="setting_scope[]"
              style="height:31px;">
              <option value="0"><mt:trans phrase="None"></option>
              <option value="1" <mt:if name="$value_name" eq="1">selected</mt:if>><mt:trans phrase="System"></option>
              <option value="2" <mt:if name="$value_name" eq="2">selected</mt:if>><mt:trans phrase="WorkSpace"></option>
              <option value="3" <mt:if name="$value_name" eq="3">selected</mt:if>><mt:trans phrase="Both" component="PluginStarter"></option>
            </select>
          </td>
          <td style="width:<mt:if name="user_language" ne="en">22%<mt:else>44%</mt:if>">
            <mt:setvarblock name="value_name">setting_labels[<mt:var name="__index__">]</mt:setvarblock>
            <input type="text" name="setting_label[]" class="form-control watch-changed form-control-sm" value="<mt:var name="$value_name" escape>">
          </td>
          <mt:if name="user_language" ne="en">
          <td style="width:22%">
            <mt:setvarblock name="value_name">setting_label_locales[<mt:var name="__index__">]</mt:setvarblock>
            <input type="text" name="setting_label_locale[]" class="form-control watch-changed form-control-sm" value="<mt:var name="$value_name" escape>">
          </td>
          </mt:if>
          <td>
          <button class="btn btn-secondary btn-sm close-plugin mt-1 mr-2" type="button" aria-label="<mt:trans phrase="Detach">">
            <span aria-hidden="true">&times;</span>
          </button>
          </td>
        </tr>
        </mt:loop>
      </tbody>
    </table>
  </div>
</div>

